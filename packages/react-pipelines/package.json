{
    "name": "@trollmoj/react-pipelines",
    "description": "A React library for building pipelines on a grid",
    "private": false,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
        "preview": "vite preview"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "styled-components": "^6.1.13"
    },
    "devDependencies": {
        "@types/node": "^20.8.2",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.3.3",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "json": "^11.0.0",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vite-plugin-dts": "^4.3.0"
    },
    "license": "MIT",
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "styled-components": "^6.1.13"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/react-pipelines.umd.js",
    "module": "./dist/react-pipelines.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/react-pipelines.es.js",
            "require": "./dist/react-pipelines.umd.js"
        }
    }
}